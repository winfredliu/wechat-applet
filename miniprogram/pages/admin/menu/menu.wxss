.menu-list {
  display: flex;
  width: 100vw;
  flex-wrap: wrap;
}

.menu-tem {
  box-sizing: border-box;
  flex: 0 0 33.33%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 192rpx;
  border-right: 1px solid #eaeef1;
  border-bottom: 1px solid #eaeef1;

}

.menu-item-text {
  font-size: 26rpx;
  margin-top: 10rpx;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}